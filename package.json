{
  "name": "city-lights-ui",
  "description": "The City Lights UI Theme is a beautiful dark matte Atom UI theme that is designed with a developer's focus in mind.",
  "repository": "https://github.com/Yummygum/city-lights-ui-atom",
  "version": "1.3.5",
  "author": "Noud Adrichem",
  "theme": "ui",
  "main": "lib/main",
  "configSchema": {
    "showTabsInTreeView": {
      "type": "boolean",
      "default": false
    },
    "fontSize": {
      "title": "UI Font Size",
      "description": "Change the font size for the UI theme.",
      "type": "integer",
      "default": 13,
      "enum": [
        9,
        10,
        11,
        12,
        13,
        14,
        15,
        16
      ]
    }
  },
  "keywords": [
    "ui",
    "theme",
    "citylights",
    "city lights",
    "darktheme",
    "dark theme",
    "city-lights-ui",
    "city-lights-syntax",
    "city-lights-icons"
  ],
  "license": "CC-BY-NC-ND",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  }
}
